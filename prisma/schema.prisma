generator doctor {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Doctor {
  id        String    @id
  name      String
  crm       String
  password  String
  createdAt DateTime
  updatedAt DateTime? @updatedAt
  deletedAt DateTime?

  @@map("doctors")
}

model SeedersHistory {
  id         String   @id
  name       String
  executedAt DateTime

  @@map("seeders_histories")
}
