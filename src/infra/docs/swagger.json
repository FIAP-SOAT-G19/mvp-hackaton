                  "doctorCrm": {
                    "type": "string",
                    "example": "123456"
                  }
                },
                "required": [
                  "startDate",
                  "doctorCrm"
                ]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Horário adicionado com sucesso",
