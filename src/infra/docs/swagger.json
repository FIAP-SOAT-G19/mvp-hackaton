{
  "openapi": "3.0.0",
  "info": {
    "title": "Health&Med",
    "description": "API desenvolvida durante o hackaton",
    "version": "1.0.0",
    "contact": {
       "email": "tech-challenge@gmail.com"
    }
  },
  "servers": [
    {
      "url": "/api/v1"
    }
  ],
  "tags": [
    {
      "name": "Healthcheck"
    },
    {
      "name": "Schedules"
    }
  ],
  "paths": {
    "/healthcheck": {
      "get": {
        "tags": [
          "Healthcheck"
        ],
        "operationId": "healthcheck",
        "description": "Rota de Healthcheck da API",
        "summary": "Retorna status da API",
        "responses": {
          "200": {
            "description": "Online",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "OK"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/client/auth": {
      "post": {
        "tags": [
          "Clients"
        ],
        "operationId": "post-auth",
        "description": "Rota de autenticação de Clientes",
        "summary": "Autentica um cliente",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "example": "jose.silva@gmail.com",
                    "description": "Este é o email do cliente"
                  },
                  "password": {
                    "type": "string",
                    "example": "J$2023",
                    "description": "Esta é a senha do cliente"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "example": "José da Silva",
                      "description": "Este é o nome do cliente"
                    },
                    "email": {
                      "type": "string",
                      "example": "jose.silva@gmail.com",
                      "description": "Este é o email do cliente"
                    },
                    "cpf": {
                      "type": "string",
                      "example": "32125869777",
                      "description": "Este é o CPF do cliente"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "InvalidParamError",
                      "description": ""
                    },
                    "message": {
                      "type": "string",
                      "example": "Invalid Param: name",
                      "description": ""
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Internal Server Error",
                      "description": ""
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/doctor/schedules": {
      "post": {
        "tags": [
          "Schedules"
        ],
        "operationId": "post-schedules",
        "description": "Rota de Agendamento do Médico da API",
        "summary": "Criar as Agendas dos Médicos da API",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "startDate": {
                    "type": "string",
                    "format": "date-time",
                    "example": "2024-07-20T09:00:00Z"
                  },
                  "doctorCrm": {
                    "type": "string",
                    "example": "123456"
                  }
                },
                "required": [
                  "startDate",
                  "doctorCrm"
                ]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Horário adicionado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "scheduleId": {
                      "type": "string",
                      "example": "5ebcea6f-2391-46aa-a5bc-ab5d98527f13"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Missing param: crm"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Internal Server Error"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Schedules"
        ],
        "operationId": "get-all-schedules",
        "description": "Rota de Agendamento do Médico da API",
        "summary": "Retorna os Agendamentos da API",
        "parameters": [
          {
            "name": "crm",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Agendamentos retornados com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                      "type": "string",
                      "example": "5ebcea6f-2391-46aa-a5bc-ab5d98527f13"
                    },
                    "startDate": {
                      "type": "string",
                      "format": "date-time",
                      "example": "2024-07-20T09:00:00Z"
                    },
                    "endDate": {
                      "type": "string",
                      "format": "date-time",
                      "example": "2024-07-20T09:50:00"
                    },
                    "doctorId": {
                      "type": "string",
                      "example": "66bcea6f-2391-46aa-a5bc-ab5d98527f13"
                    },
                    "doctorName": {
                      "type": "string",
                      "example": "José da Silva"
                    },
                    "doctorCrm": {
                      "type": "string",
                      "example": "123456"
                    }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Internal Server Error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/doctor/schedules/{scheduleId}": {
      "get": {
        "tags": [
          "Schedules"
        ],
        "operationId": "get-schedule",
        "description": "Rota de Agendamento do Médico da API",
        "summary": "Retorna as informações de um Agendamento da API",
        "parameters": [
          {
            "in": "path",
            "name": "scheduleId",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "ID do agendamento"
          }
        ],
        "responses": {
          "200": {
            "description": "Informações do Agendamento retornadas com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "example": "5ebcea6f-2391-46aa-a5bc-ab5d98527f13"
                    },
                    "startDate": {
                      "type": "string",
                      "format": "date-time",
                      "example": "2024-07-20T09:00:00Z"
                    },
                    "endDate": {
                      "type": "string",
                      "format": "date-time",
                      "example": "2024-07-20T09:50:00"
                    },
                    "doctorId": {
                      "type": "string",
                      "example": "66bcea6f-2391-46aa-a5bc-ab5d98527f13"
                    },
                    "doctorName": {
                      "type": "string",
                      "example": "José da Silva"
                    },
                    "doctorCrm": {
                      "type": "string",
                      "example": "123456"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Schedule not found error"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Internal Server Error"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "patch": {
        "tags": [
          "Schedules"
        ],
        "operationId": "patch-schedule",
        "description": "Rota de Agendamento do Médico da API",
        "summary": "Atualiza as informações de um Agendamento da API",
        "parameters": [
          {
            "in": "path",
            "name": "scheduleId",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "ID do agendamento"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "doctorCrm": {
                    "type": "string",
                    "example": "123456"
                  },
                  "startDate": {
                    "type": "date",
                    "example": "2024-07-20T09:00:00"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Atualização do agendamento realizada com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "doctorCrm": {
                      "type": "string",
                      "example": "123456"
                    },
                    "startDate": {
                      "type": "date",
                      "example": "2024-07-20T09:00:00"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Schedule not found error"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Internal Server Error"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "Schedules"
        ],
        "operationId": "delete-schedule",
        "description": "Rota de Agendamento do Médico da API",
        "summary": "Deleta um Agendamento da API",
        "parameters": [
          {
            "in": "path",
            "name": "scheduleId",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "ID do agendamento"
          }
        ],
        "responses": {
          "200": {
            "description": "Agendamento deletado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Schedule deleted"
                    },
                    "scheduleId": {
                      "type": "string",
                      "example": "ca97b811-a559-44b0-b3b0-1a9a84309aef"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Schedule not found error"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Internal Server Error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}