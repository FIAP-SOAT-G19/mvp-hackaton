    "/doctor/auth": {
      "post": {
        "tags": [
          "Doctor"
        ],
        "operationId": "auth-doctor",
        "description": "Rota de Autenticação do Médico na API",
        "summary": "Autentica o Médico na API",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "crm": {
                    "type": "string",
                    "example": "000000/SP"
                  },
                  "password": {
                    "type": "string",
                    "example": "123456"
                  }
                },
                "required": [
                  "crm",
                  "password"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Médico autenticado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "example": "João da Silva"
                    },
                    "crm": {
                      "type": "string",
                      "example": "000000/UF"
                    },
                    "token": {
                      "type": "string",
                      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxMjM0NTY3ODkwIiwibmFtZSI6IkpvaG4gRG9lIiwiaWF0IjoxNTE2MjM5MDIyfQ.SflKxwRJSMeKKF2QT4fwpMeJf36POk6yJV_adQssw5c"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Missing param: crm or password"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Internal Server Error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/doctor": {
